{
  "repository": "/mnt/backups/restic",
  "passwordFile": "/etc/restic-password",
  "defaults": {
    "type": "volume"
  },
  "jobs": [
    {
      "name": "linkding",
      "type": "bind",
      "targets": ["/mnt/swarm-data/linkding"],
      "pre_stop": "linkding-swarm_linkding",
      "post_start": "linkding-swarm_linkding"
    },
    {
      "name": "karakeep",
      "type": "bind",
      "targets": ["/mnt/swarm-data/karakeep"],
      "pre_stop": "karakeep-swarm_karakeep",
      "post_start": "karakeep-swarm_karakeep"
    },
    {
      "name": "memos",
      "type": "volume",
      "targets": ["memos-data"]
    },
    {
      "name": "ntfy",
      "type": "volume",
      "targets": ["ntfy-data", "ntfy-cache"]
    },
    {
      "name": "adguardhome",
      "type": "volume",
      "targets": ["dozzle_adguardhome-conf", "dozzle_adguardhome-work"]
    },
    {
      "name": "caddy",
      "type": "bind",
      "targets": ["/home/crussell/caddy/Caddyfile"],
      "pre_stop": "caddy-proxy",
      "post_start": "caddy-proxy"
    },
    {
      "name": "immich-postgres",
      "type": "volume",
      "targets": ["immich-swarm_postgres_data"],
      "pre_stop": "immich-swarm_postgres",
      "post_start": "immich-swarm_postgres"
    },
    {
      "name": "searxng",
      "type": "volume",
      "targets": ["searxng_searxng-valkey-data"],
      "pre_stop": "searxng_searxng",
      "post_start": "searxng_searxng"
    }
  ]
}
